<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-clsprint" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>clsPrint</name>
	<js-module name="clsPrint" src="www/clsPrint.js">
		<clobbers target="cordova.plugins.clsPrint" />
	</js-module>
	<platform name="android">
		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="clsPrint">
				<param name="android-package" value="cordova-plugin-clsprint.clsPrint" />
			</feature>
		</config-file>
		<config-file parent="/*" target="AndroidManifest.xml"></config-file>
		<source-file src="src/android/clsPrint.java" target-dir="src/cordova-plugin-clsprint/clsPrint" />
		<source-file src="src/android/CSJLabelLib_Android.jar" target-dir="libs" />
	</platform>
</plugin>